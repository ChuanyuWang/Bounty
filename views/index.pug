extends layout

block append css
  link(rel="stylesheet", href="//cdn.bootcss.com/material-design-icons/3.0.1/iconfont/material-icons.min.css")
  link(rel="stylesheet", href=CDN_FILES.vuetify_css)

block append js
  if ENV_DEVELOPMENT
    script(src="//cdn.bootcss.com/jquery/3.2.1/jquery.js")
    script(src=CDN_FILES.momentjs_dev)
    script(src=CDN_FILES.i18next_dev)
    script(src=CDN_FILES.vue_dev)
    script(src=CDN_FILES.vuetify_dev)
  else
    script(src="//cdn.bootcss.com/jquery/3.2.1/jquery.min.js")
    script(src=CDN_FILES.momentjs)
    script(src=CDN_FILES.i18next)
    script(src=CDN_FILES.vue)
    script(src=CDN_FILES.vuetify)
  script(src='/js/main.js')

block content
  div#app